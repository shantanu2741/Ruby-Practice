arr = [1,2,3,4,5,6,7,8]

start = 0
end1 = arr.length-1
      target = 3
      while(start<=end1)
        mid=(start+end1)/2
          if arr[mid] == target 
              p mid
            break
          elsif arr[mid]> target 
              end1=mid-1 
          else
              start=mid+1
       end
 end
